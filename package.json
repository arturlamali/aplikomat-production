{
	"name": "aplikomat",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "next lint && tsc --noEmit",
		"db:generate": "drizzle-kit generate --config drizzle.config.postgres.ts",
		"db:generate:postgres": "pnpm db:generate drizzle.config.postgres.ts",
		"db:generate:sqlite": "pnpm db:generate drizzle.config.sqlite.ts",
		"db:migrate": "drizzle-kit migrate --config drizzle.config.postgres.ts",
		"db:migrate:postgres": "pnpm db:migrate drizzle.config.postgres.ts",
		"db:migrate:sqlite": "pnpm db:migrate drizzle.config.sqlite.ts",
		"db:push": "drizzle-kit push --config drizzle.config.postgres.ts",
		"db:push:postgres": "pnpm db:push drizzle.config.postgres.ts",
		"db:push:sqlite": "pnpm db:push drizzle.config.sqlite.ts",
		"db:studio": "drizzle-kit studio --config drizzle.config.postgres.ts",
		"db:studio:postgres": "pnpm db:studio drizzle.config.postgres.ts",
		"db:studio:sqlite": "pnpm db:studio drizzle.config.sqlite.ts",
		"dev": "next dev --turbo",
		"format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
		"format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"preview": "next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit"
	},
